<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN"
        "http://www.w3.org/TR/html4/loose.dtd">
<html lang="en">

<head>
	<meta http-equiv="content-type" content="text/html; charset=utf-8">
	<title>Cycling Log | Select Cyclist</title>
  <link rel="stylesheet" href="/cycling_log/static/style/cycling_log.css">
</head>

<body>
<form method="post" action="/cycling_log/dynamic/workouts/home">
<table cellspacing="0" cellpadding="0" class="table_content">
  <tr>
    <td valign="top" height="*">
      <table cellspacing="0" cellpadding="0" width="100%">
        <tr class="tab_row">
            <td class="tab_empty_space"></td>
        </tr>
      </table>
    </td>
  </tr>
  <tr>
    <td height="100%" class="tabbed_table">
      <table border="1">
        <tr>
          <td>
            <span class="top_label">Cyclist</span>
          </td>
        </tr>
        <tr>
          <td>
            <span class="cyclist_select">#select("cyclist" ["sw", "kevin", "cheryl"] "$!{cyclist}")</span>
            <input type="image" name="selectCyclist" src="/cycling_log/static/images/buttons/right.gif" class="right_button">
          </td>
        </tr>
      </table>
    </td>
  </tr>
</table>
</form>

</body>
</html>
